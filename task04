#include <iostream>
using namespace std;

int main()
{
    double A, B, C;

    cout << "Введіть розмір A: ";
    cin >> A;
    cout << "Введіть розмір B: ";
    cin >> B;
    cout << "Введіть розмір C: ";
    cin >> C;

    double surface = 2 * (A * B + B * C + A * C);
    double results = A * B * C;

    cout << "Площа поверхні паралелепіпеда: " << surface << endl;
    cout << "Об'єм паралелепіпеда: " << results << endl;
}
